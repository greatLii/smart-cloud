<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smart.cloud.mapper.UserMapper">
    <select id="getUserInfoByUUid" resultType="com.smart.cloud.entity.UserEntity">
        SELECT *
        FROM smart_user
        WHERE uuid = #{uuid}
    </select>
</mapper>